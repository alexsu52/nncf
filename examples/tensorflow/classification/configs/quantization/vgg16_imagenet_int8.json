{
    "model": "VGG16",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 224, 224, 3]
    },
    "batch_size": 128,
    "epochs": 10,
    "optimizer": {
        "type": "Adam",
        "schedule_type": "piecewise_constant",
        "schedule_params": {
            "boundaries": [5],
            "values": [1e-5, 1e-6]
        }
    },
    "dataset": "imagenet2012",
    "dataset_type": "tfds",

   "compression": {
        "algorithm": "quantization",
        "initializer": {
            "range": {
                "type": "min_max"
            }
        },
        "disable_saturation_fix": true
    }
}
